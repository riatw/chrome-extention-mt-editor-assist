{
	"manifest_version": 2,
	"name": "Movable Type Editor Assist",
	"description": "公開サイト側から対応するCMS管理画面に誘導する拡張機能です。（It is an extension to induce from the public site side to the corresponding CMS management screen.）",
	"version": "1.0.12",
	"icons": {
		"128": "images/icon/icon_128.png"
	},
	"browser_action": {
		"default_icon": {
			"128": "images/icon/icon_128.png"
		},
		"default_title": "Movable Type Editor Assist",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab"
	],
    "options_page": "options.html",
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}